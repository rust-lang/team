{
  "org": "rust-lang",
  "name": "cargo",
  "description": "The Rust package manager",
  "homepage": "https://doc.rust-lang.org/cargo",
  "bots": [
    "rustbot",
    "rfcbot",
    "renovate"
  ],
  "teams": [
    {
      "name": "cargo",
      "permission": "write"
    }
  ],
  "members": [],
  "branch_protections": [
    {
      "pattern": "master",
      "dismiss_stale_review": false,
      "mode": {
        "pr_required": {
          "ci_checks": [
            "conclusion"
          ],
          "required_approvals": 1
        }
      },
      "allowed_merge_teams": [],
      "merge_bots": []
    },
    {
      "pattern": "rust-1.*",
      "dismiss_stale_review": false,
      "mode": {
        "pr_required": {
          "ci_checks": [
            "conclusion"
          ],
          "required_approvals": 1
        }
      },
      "allowed_merge_teams": [],
      "merge_bots": []
    }
  ],
  "archived": false,
  "private": false,
  "auto_merge_enabled": true
}